<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TranslateTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/colorTranslate</item>
        <item name="colorPrimaryDark">@color/colorTranslateDark</item>
        <item name="colorAccent">@color/colorTranslateAccent</item>
    </style>

    <style name="DictionaryTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/colorDictionary</item>
        <item name="colorPrimaryDark">@color/colorDictionaryDark</item>
        <item name="colorAccent">@color/colorDictionaryAccent</item>
    </style>

</resources>
